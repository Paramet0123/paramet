foods= []
prices=[]
total=0

while True:
    food= input("Enter a food to but (q to quit): ")
    if food.lower()=="q":
        break
    else:
        price=float(input(f"Enter the pirce of a {food}: $"))
        foods.append(food)
        prices.append(price)

print("-----YOUR CART ----")
for food in foods:
    print(food ,end=" " )

for price in prices:
    total +=price
print()
print(f"Your total is : $ {total}")

