import time  #time import 
my_time = int(input("Enter the time in seconds: ")) #‡∏õ‡πâ‡∏≠‡∏ô‡∏Ç‡πâ‡∏≠‡∏°‡∏ß‡∏•
for x in reversed(range(0,my_time)): #‡πÄ‡∏£‡∏¥‡πà‡∏° 0-time 
    seconds=x%60 #
    minutes= int(x/60)%60 #
    hours =int(x/3600) #
    print(f"{hours:02}:{minutes:02}:{seconds:02}")  #‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÄ‡∏´‡∏•‡∏∑‡∏≠2 ‡∏ó‡∏®
    time.sleep(1) #‡∏´‡∏ô‡∏∂‡πà‡πà‡∏á == ‡∏≠‡∏≠‡∏Å‡∏à‡∏≤‡∏Å‡∏£‡∏π‡∏õ
